<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>AgileAssistant.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>LoginService</servlet-name>
  	<servlet-class>com.csci.finalproject.agileassistant.server.LoginServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginService</servlet-name>
  	<url-pattern>/agileassistant/LoginService</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>UserStoryService</servlet-name>
  	<servlet-class>com.csci.finalproject.agileassistant.server.UserStoryServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserStoryService</servlet-name>
  	<url-pattern>/agileassistant/UserStoryService</url-pattern>
  </servlet-mapping>

</web-app>
